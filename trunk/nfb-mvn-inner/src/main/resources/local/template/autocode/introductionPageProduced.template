<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>痕迹</title>
<link rel="stylesheet" type="text/css" href="../nfb.css">
</head>
<body>
	<h1>@{GetCaption}</h1>
	<p>@{GetDesc}</p>
	<h2>实体类  (@{beanName})</h2>
	<div id="entity">
		<table>
			<thead>
				<tr>
					<td width="10%">序号</td>
					<td width="20%">字段</td>
					<td width="20%">名称</td>
					<td width="15%">数据类型</td>
					<td width="35%">备注</td>
				</tr>
			</thead>
			<tbody>
				@{GetEntityField}
			</tbody>
		</table>
	</div>
	<h2>控制层 (@{beanName}Controller)</h2>
	<div id="entity">
		<table>
			<thead>
				<tr>
					<td width="10%">序号</td>
					<td width="20%">URL</td>
					<td width="25%">方法</td>
					<td width="35%">备注</td>
				</tr>
			</thead>
			<tbody>
			   @{GetControllerField}
			</tbody>
		</table>
	</div>
	<h2>服务层 (@{beanName}Service)</h2>
	<div id="entity">
		<table>
			<thead>
				<tr>
					<td width="10%">序号</td>
					<td width="20%">方法</td>
					<td width="35%">备注</td>
				</tr>
			</thead>
			<tbody>
			  @{GetServiceField}
			</tbody>
		</table>
	</div>
	<h2>数据层(@{beanName}Dao)</h2>
	<div id="entity">
		<table>
			<thead>
				<tr>
					<td width="10%">序号</td>
					<td width="20%">方法</td>
					<td width="35%">备注</td>
				</tr>
			</thead>
			<tbody>
			   @{GetDaoField}
			</tbody>
		</table>
	</div>
</body>
</html>